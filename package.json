{
  "name": "topify",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "dev": "vite --port ${PORT:-5000}",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint --quiet --fix",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "upgrade:yarn": "yarn set version latest",
    "upgrade": "yarn upgrade-interactive ; yarn upgrade:yarn",
    "dev:spotify": "nodemon --exec 'esr --cache' --ext 'ts' lib/spotify/test.ts",
    "check": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@geist-ui/icons": "^1.0.2",
    "cross-fetch": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^3.2.27",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vercel": "^39.1.2",
    "vite": "^6.0.9",
    "vite-plugin-vercel": "^9.0.3"
  },
  "license": "MIT",
  "packageManager": "yarn@4.5.3"
}
