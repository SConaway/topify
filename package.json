{
  "name": "topify",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "dev": "vite --port ${PORT:-5000}",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint --quiet --fix",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "upgrade:yarn": "yarn set version latest",
    "upgrade": "yarn upgrade-interactive ; yarn upgrade:yarn",
    "dev:spotify": "nodemon --exec 'tsx' --ext 'ts' lib/spotify/test.ts",
    "check": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@geist-ui/icons": "^1.0.2",
    "cross-fetch": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.3",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.9",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vercel/node": "^5.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vercel": "^39.3.0",
    "vite": "^6.0.11",
    "vite-plugin-vercel": "^9.0.4"
  },
  "license": "MIT",
  "packageManager": "yarn@4.6.0"
}
